#include <stdint.h>
#include "multiplication.oh"
#include "/home/paul/absentminded-crypto-kit/src/obig.oh"

void oblivMul(void *args) obliv 
{

	ProtocolIO *mul = (ProtocolIO*)args;

	obliv int oa = feedOblivInt(mul->num1, 1);
	obliv int ob = feedOblivInt(mul->num2, 2); 
	obliv int oc = 0;
	obig_mul(&oc, oa, ob);

 	revealOblivInt(&mul->result, oc, 0);
	
}	